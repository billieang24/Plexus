# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.main()
GET     /forgotPassword             controllers.Application.forgotPassword()
GET     /profile                    controllers.Application.profile()
GET     /signUp                     controllers.Application.signUp()
GET     /homePage                   controllers.Application.homePage()
GET     /verification               controllers.Application.verificationPage()
GET     /addFriend                  controllers.Application.addFriend()
GET     /photoUpload               	controllers.Application.photoUploadPage()
GET		/friends					controllers.Application.friends()
GET		/about						controllers.Application.about()
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
